<h1>Children</h1>

<h2><%= link_to 'create a new child', new_account_child_path %></h2>

<% @children.each do |child| %>
  <% if child.user_id == current_user.id %>
    <li>
    <%= link_to child.name, child_path(child) %>
    <%= link_to 'delete', account_child_path(child), method: :delete %>
    </li>
  <% end %>
<% end %>


